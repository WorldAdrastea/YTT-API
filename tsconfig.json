{
    "compilerOptions": {
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "baseUrl": "src",
        "outDir": "./dist",
        "typeRoots": ["./node_modules/@types"],
        "types": ["node", "express", "lodash"],
        "sourceMap": true,
        "noImplicitAny": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
    },
    "include": ["src/**/*"],
}