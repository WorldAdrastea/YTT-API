{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "baseUrl": "src",
        "outDir": "./dist",
        "types": [],
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules/@types/node",
            "node_modules/@types",
        ],
        "sourceMap": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
    },
    "include": ["src/**/*"],
}