{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "baseUrl": "src",
        "outDir": "./dist",
        "types": ["node"],
        "typeRoots": [
            "./node_modules/@types"
        ],
        "sourceMap": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
    },
    "include": ["src/**/*"],
}